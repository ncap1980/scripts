--action test \
--def "movieFormat={fn =~ /4k|4K|2160p|2160P/ ? '/volume1/14Teras/Multimedia/4K' : {genres.contains('Familia') ? '/volume1/14Teras/Multimedia/Dibujos' : genres.contains('japan') ? '/volume1/14Teras/Multimedia/Anime' : genres.contains('Terror') ? '/volume1/14Teras/Multimedia/Terror' : genres.contains('Documental') ? '/volume1/14Teras/Multimedia/Documentales' : '/volume1/14Teras/Multimedia/Peliculas'}}/{n} ({y}) {vf}" \
